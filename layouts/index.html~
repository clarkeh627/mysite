			{{ $last := eq $index (sub (len $list) 1) }}
			{{ if and $.Site.Params.footer.enable $last }}
				{{"<!-- Footer -->" | safeHTML}}
				<section id="footer">
			{{ end }}
			{{ if $last }}
				{{ partial (print $element.type ".html") (dict "block" $element "params" $.Site.Params) }}
